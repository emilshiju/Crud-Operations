<div class="d-flex justify-content-between flex-wrap-md nowrap align-items-center pt-3 pb-2 mb-3 border-bottom">

    <h1 class="h2">{{customers.firstName}} {{customers.lastName}}</h1>

    <div class="btn-toolbar mb-2 mb-md 0">
        <div class="btn-group me-2">
            {{!-- <a href="/add" class="btn btn-sm btn-outline-secondary">+ new user</a> --}}
            <button type="submit" class="btn btn-sm btn-outline-secondary">share</button>
             <button type="submit" class="btn btn-sm btn-outline-secondary">export</button>
        </div>
    </div>
</div>


<div class="col py-3">
    <div class="row">
        <div class="col">
            <nav aria-label="breadcrumb">
        <ol class="breadcrumb">
          <li class="breadcrumb-item"><a href="/">Dashboard</a></li>
          <li class="breadcrumb-item active">{{customers.firstName}}</li>
        </ol>
      </nav>
        </div>
        <div class="col text-end tw-lighter">
            {{!-- <h5>lastupdate{{ new Date(customers.updateAt).toUTCString()}}</h5> --}}
            <h5>Last Update: {{customers.updateAt}}</h5>


        </div>
    </div>
</div>

<ul class="list-group">
    <li class="list-group-item d-flex justify-content-between align-items-center">
        <div class="col-4"><b>Name:</b></div>
        <div class="col">{{customers.firstName}} {{customers.lastName}}</div>
    </li>
    <li class="list-group-item d-flex justify-content-between align-items-center">
        <div class="col-4"><b>password:</b></div>
        <div class="col">{{customers.password}}</div>
    </li>
    <li class="list-group-item d-flex justify-content-between align-items-center">
        <div class="col-4"><b>Email:</b></div>
        <div class="col">{{customers.email}}</div>
    </li>
    <li class="list-group-item d-flex justify-content-between align-items-center">
        <div class="col-4"><b>CreatedAt:</b></div>
        <div class="col">{{customers.createAt}}</div>
    </li>
    <li class="list-group-item d-flex justify-content-between align-items-center">
        <div class="col-4"><b>UpdatedAt:</b></div>
        <div class="col">{{customers.updateAt}}</div>
    </li>
</ul>
